# nginx configuration
server {
    listen 80;
    server_name 3.84.222.117;

    location / {
        try_files $uri $uri/ =404;
        include proxy_params;
        proxy_pass http://127.0.0.1:8000;
    }

    location /airbnb-onepage/ {
        rewrite ^/airbnb-onepage(.*)$ $1 break;
        include proxy_params;
        proxy_pass http://127.0.0.1:8000;
    }
}
